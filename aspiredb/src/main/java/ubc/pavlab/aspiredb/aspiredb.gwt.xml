<!-- $Id: aspiredb.gwt.xml,v 1.18 2013/06/14 18:56:54 cmcdonald Exp $ -->
<module rename-to='aspiredb'>
	<!-- Inherit the core Web Toolkit stuff. -->	
	<inherits name="com.google.gwt.http.HTTP" />
	
	<!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/> -->
	<inherits name='com.google.gwt.user.theme.standard.Standard'/>
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->
	<inherits name='com.google.gwt.uibinder.UiBinder' />
	<inherits name="com.sencha.gxt.ui.GXT" />
	
	<inherits name="com.googlecode.gwtx.Java"></inherits>
	
	<!-- Specify the app entry point class. -->
	<entry-point class='ubc.pavlab.aspiredb.client.aspiredb' />

	<set-configuration-property name='CssResource.style' value='pretty' />

	<source path="client" />
	<source path="shared" />
	
	<!--  for super dev mode uncomment
	<add-linker name="xsiframe"/>
	<set-configuration-property name="devModeRedirectEnabled" value="true"/>
-->
</module>