<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:gx="urn:import:com.google.gwt.canvas.client">

	<ui:style>
		.ideogram {z-index: 0; position:absolute; top:20; left:0;}
        .ideogramOverlay {z-index: 1; position:absolute; top:20; left:0;}
	</ui:style>

	<g:ScrollPanel>
        <g:FlowPanel>
		    <gx:Canvas ui:field="canvasBox" width="600px" height="600px" styleName="{style.ideogram}"/>
            <gx:Canvas ui:field="overlayCanvasBox" width="600px" height="600px" styleName="{style.ideogramOverlay}"/>
        </g:FlowPanel>
	</g:ScrollPanel>

	
</ui:UiBinder> 