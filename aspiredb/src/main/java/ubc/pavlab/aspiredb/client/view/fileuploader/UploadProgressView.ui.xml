<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:view='urn:import:ubc.pavlab.aspiredb.client.view.fileuploader'>

    <g:HTMLPanel>
        <div class="Wrapper">
            <div class="Container">
                <div class="ContainerHead">
                  Upload File    
                </div>
                <div class="ContainerBody">
                    <view:FileSubmit ui:field='fileSubmit'/>               
                    <view:UploadProgress ui:field='uploadProgress'/>  
                </div>
            </div>
           
            <div class="Container">
                <div class="ContainerHead">
                  Uploaded Files
                </div>
                <div class="ContainerBody">
                    <view:FileList ui:field='fileList'/>
                    <div class="Wrapper">
                        <view:Paging ui:field='paging'/>  
                    </div>
                </div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>